{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./src",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2015",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": ["node_modules/@types"],
    "lib": ["es2018", "dom"],
    "paths": {
      "@models*": ["app/core/models/index"],
      "@services*": ["app/core/services/index"],
      "@reducers*": ["app/redux/reducers"],
      "@actions*": ["app/redux/actions"],
      "@share*": ["app/share*"]
    }
  }
}
